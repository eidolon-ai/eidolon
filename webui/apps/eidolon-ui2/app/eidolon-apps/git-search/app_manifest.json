{
  "name": "Explore Github docs",
  "description": "A tool to explore the Github documentation using GPT-4-latest and RAG",
  "version": "0.1.0",
  "image": "image.webp",
  "location": "http://localhost:8080",
  "type": "copilot",
  "params": {
    "custom_page": "git-search",
    "agent": "repo_expert",
    "operation": "converse",
    "inputLabel": "How can I help you?",
    "titleOperationName": "generate_title",
    "allowSpeech": true,
    "speechAgent": "speech_agent",
    "speechOperation": "speech_to_text"
  }
}
