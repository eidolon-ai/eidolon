{
  "name": "eidolon-ui2",
  "version": "0.0.1",
  "description": "Eidolon UI app",
  "author": {
    "name": "Dave Brewster",
    "email": "dave@augustdata.ai",
    "url": "https://augustdata.ai"
  },
  "private": true,
  "keywords": [
    "nextjs",
    "react",
    "typescript",
    "mui",
    "material",
    "material ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eidolon-ai/eidolon"
  },
  "scripts": {
    "dev": "(patch -N ../../node_modules/.pnpm/@jsdevtools+ono@7.1.3/node_modules/@jsdevtools/ono/esm/index.js ono_turbo.patch || true) && npm run build-eidolon && next dev --turbo",
    "build": "npm run build-eidolon && next build",
    "build:static": "npm run build-eidolon && next build && next export",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build-sdk": "docker build -t eidolonai/sdk_base:latest -f ../../../sdk/Dockerfile ../../../sdk/",
    "docker:build": "npm run docker:build-sdk && docker-compose build",
    "docker:rebuild": "pnpm run docker:down && pnpm run docker:build && pnpm run docker:up",
    "test:e2e": "pnpm run docker:rebuild && pnpm exec playwright test && pnpm run docker:down",
    "playwright:debug": "pnpm exec playwright test --debug",
    "format": "prettier ./ --write",
    "lint": "next lint",
    "start": "next start",
    "type": "tsc",
    "build-eidolon": "node eidolon-app.build.js"
  },
  "dependencies": {
    "@eidolon/client": "workspace:*",
    "@eidolon/components": "workspace:*",
    "@eidolon/usage-client": "workspace:*",
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/server": "latest",
    "@emotion/styled": "latest",
    "@jsdevtools/ono": "latest",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/faker": "^6.6.9",
    "@types/qs": "^6.9.15",
    "clsx": "latest",
    "copy-to-clipboard": "latest",
    "next": "14.2.3",
    "next-auth": "beta",
    "qs": "^6.12.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.1",
    "@testing-library/jest-dom": "latest",
    "@testing-library/react": "latest",
    "@testing-library/user-event": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "babel-jest": "latest",
    "eslint": "8.57.0",
    "eslint-config-next": "^14.2.3",
    "eslint-plugin-testing-library": "latest",
    "jest": "latest",
    "jest-environment-jsdom": "latest",
    "next-router-mock": "latest",
    "playwright": "^1.44.1",
    "prettier": "latest",
    "typescript": "latest"
  }
}
